  "external-ui-url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
  "external-ui": "ui",
  "external-controller-cors": { "allow-origins": ["*"], "allow-private-network": true },
  "profile": { "store-selected": true, "store-fake-ip": true },
  "dns": {
    "enable": true,
    "listen": "0.0.0.0:1053",
